<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<table>
  <thead>
    <tr>
      <th>Venueid</th>
      <th>Venuename</th>
      <th>Venueaddress</th>
      <th>Venuecity</th>
      <th>Venuestate</th>
      <th>Venuezip</th>
      <th>Venuephone</th>
      <th>Venueage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.venueid %></td>
        <td><%= venue.venueName %></td>
        <td><%= venue.venueAddress %></td>
        <td><%= venue.venueCity %></td>
        <td><%= venue.venueState %></td>
        <td><%= venue.venueZip %></td>
        <td><%= venue.venuePhone %></td>
        <td><%= venue.venueAge %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
